
<table border="1" cellpadding="5">
  <tr>
    <th>Nombres</th>
    <th>Apellidos</th>
    <th>Fecha Nacimiento</th>
    <th>Dirección</th>
    <th>Celular</th>
    <th>Correo</th>
    <th>Acciones</th>
  </tr>

  <% agenda.forEach(contacto => { %>
    <tr>
      <td><%= contacto.nombres %></td>
      <td><%= contacto.apellidos %></td>
      <td><%= contacto.fecha_nacimiento.toISOString().split('T')[0] %></td>
      <td><%= contacto.direccion %></td>
      <td><%= contacto.celular %></td>
      <td><%= contacto.correo %></td>
      <td>
        <a href="/edit/<%= contacto.id %>">Editar</a> |
        <a href="/delete/<%= contacto.id %>" onclick="return confirm('¿Eliminar contacto?')">Eliminar</a>
      </td>
    </tr>
  <% }) %>
</table>
